{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Home\",\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        //有id是更新，没有是新增\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: '更新成功'\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: '新增成功'\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n    handelEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleDelete() {},\n    handleSizeChange(PageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = PageSize;\n      this.load();\n    },\n    handleCurrentChange(PageNum) {\n      //改变当前页码出发\n      this.currentPage = PageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","get","params","pageNum","then","res","console","log","records","add","save","id","put","code","$message","type","message","msg","post","handelEdit","row","JSON","parse","stringify","handleDelete","handleSizeChange","PageSize","handleCurrentChange","PageNum"],"sources":["D:\\CODING\\IDEA-coding\\SpringDemo\\SpringBootVue\\vue\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <el-container>\r\n    <el-header style=\"height: 100px;text-align: left\">\r\n      <div style=\"height: 50px;margin-top: 20px\">\r\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"add\">新增</el-button>\r\n        <el-button type=\"primary\">导入</el-button>\r\n        <el-button type=\"primary\">导出</el-button>\r\n      </div>\r\n<!--      搜索区域-->\r\n      <div style=\"height: 30px\">\r\n        <el-input v-model=\"search\" style=\"width: 240px\" placeholder=\"Please input\" clearable/>\r\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"load\">查询</el-button>\r\n      </div>\r\n\r\n    </el-header>\r\n    <el-main>\r\n      <el-table\r\n          :data=\"tableData\" border style=\"width: 1200px\"\r\n          stripe\r\n      >\r\n        <el-table-column prop=\"id\" label=\"ID\" style=\"width: 100px\" />\r\n        <el-table-column prop=\"username\" label=\"用户名\" style=\"width: 200px\" />\r\n        <el-table-column prop=\"nickname\" label=\"昵称\" style=\"width: 200px;\"/>\r\n        <el-table-column prop=\"age\" label=\"年龄\" style=\"width: 100px\"/>\r\n        <el-table-column prop=\"sex\" label=\"性别\" style=\"width: 50px\" />\r\n        <el-table-column fixed=\"right\" label=\"Operations\" min-width=\"80px\">\r\n          <template #default = \"scope\">\r\n            <el-popconfirm title=\"Are you sure to delete this?\">\r\n              <template #reference>\r\n                <el-button link type=\"primary\" size=\"small\">删除</el-button>\r\n              </template>\r\n            </el-popconfirm>\r\n            <el-button link type=\"primary\" size=\"small\" @click=\"handelEdit(scope.row)\">编辑</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div class=\"demo-pagination-block\" style=\"margin-top: 10px\">\r\n        <div class=\"demonstration\" style=\"margin-bottom: 10px\">All combined</div>\r\n        <el-pagination\r\n            v-model:current-page=\"currentPage\"\r\n            v-model:page-size=\"pageSize\"\r\n            :page-sizes=\"[1, 2, 4, 8]\"\r\n            :size=\"size\"\r\n            :disabled=\"disabled\"\r\n            :background=\"background\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n        />\r\n      </div>\r\n    </el-main>\r\n\r\n\r\n  </el-container>\r\n  <el-dialog\r\n      v-model=\"dialogVisible\"\r\n      title=\"Tips\"\r\n      width=\"500px\"\r\n      :before-close=\"handleClose\"\r\n  >\r\n    <el-form :model=\"form\" label-width=\"120px\" style=\"max-width: 600px\">\r\n      <el-form-item label=\"用户名\">\r\n        <el-input v-model=\"form.username\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"昵称\" >\r\n        <el-input v-model=\"form.nickname\" style=\"width: 300px\" />\r\n      </el-form-item>\r\n<!--      <el-form-item label=\"出生日期\" style=\"width: 200px\">-->\r\n<!--        <el-col :span=\"11\">-->\r\n<!--          <el-date-picker-->\r\n<!--              v-model=\"form.date\"-->\r\n<!--              type=\"date\"-->\r\n<!--              placeholder=\"Pick a date\"-->\r\n<!--              style=\"width: 150px\"-->\r\n<!--          />-->\r\n<!--        </el-col>-->\r\n<!--        <el-col :span=\"2\" class=\"text-center\">-->\r\n<!--          <span class=\"text-gray-500\">-</span>-->\r\n<!--        </el-col>-->\r\n<!--      </el-form-item>-->\r\n      <el-form-item label=\"性别\">\r\n        <el-radio-group v-model=\"form.sex\">\r\n          <el-radio value=\"man\">男</el-radio>\r\n          <el-radio value=\"women\">女</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"save\">Create</el-button>\r\n        <el-button>Cancel</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <div class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\">\r\n          Confirm\r\n        </el-button>\r\n      </div>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  components: {\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      form:{},\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n  methods: {\r\n    load(){\r\n      request.get(\"/user\",{\r\n        params:{\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search,\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    add(){\r\n      this.dialogVisible = true\r\n      this.form = {}\r\n    },\r\n    save(){\r\n      if(this.form.id){ //有id是更新，没有是新增\r\n        request.put(\"/user\",this.form).then(res => {\r\n          console.log(res)\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type: 'success',\r\n              message: '更新成功'\r\n            });\r\n          }else{\r\n            this.$message({\r\n              type: 'error',\r\n              message: res.msg\r\n            });\r\n          }\r\n          this.load() //刷新表格数据\r\n          this.dialogVisible = false  //关闭弹窗\r\n        })\r\n      }else{\r\n        request.post(\"/user\",this.form).then(res => {\r\n          console.log(res)\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type: 'success',\r\n              message: '新增成功'\r\n            });\r\n          }else{\r\n            this.$message({\r\n              type: 'error',\r\n              message: res.msg\r\n            });\r\n          }\r\n          this.load() //刷新表格数据\r\n          this.dialogVisible = false  //关闭弹窗\r\n        })\r\n      }\r\n    },\r\n    handelEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n    },\r\n    handleDelete(){\r\n\r\n    },\r\n    handleSizeChange(PageSize) {    //改变当前每页的个数触发\r\n      this.pageSize = PageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(PageNum) { //改变当前页码出发\r\n      this.currentPage = PageNum\r\n      this.load()\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAwGA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAEZ,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAC,CAAC,CAAC;MACPC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC,CAAC;EACb,CAAC;EACDC,OAAO,EAAE;IACPD,IAAIA,CAAA,EAAE;MACJZ,OAAO,CAACc,GAAG,CAAC,OAAO,EAAC;QAClBC,MAAM,EAAC;UACLC,OAAO,EAAE,IAAI,CAACT,WAAW;UACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBF,MAAM,EAAE,IAAI,CAACA;QACf;MACF,CAAC,CAAC,CAACW,IAAI,CAACC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG;QACf,IAAI,CAACR,S